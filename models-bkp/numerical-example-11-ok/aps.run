# >> Minimizes team displacement distance and need for new teams
#############################################################################
# ampl aps.run

# ===============================================
# modelo e dados a serem usados
# ===============================================
reset data;
model aps.mod;
# data aps.dat;
data LS.dat;
data LS_distdur.dat;


# check;
